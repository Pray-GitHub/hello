pipeline{
    agent any
stages {
        stage('Build') {
            steps   {
                echo 'Building..'
                    }
            }//Build
       
        stage('Test') {
            steps {
                echo 'Testing'
                    }
                }//Testing
        stage('Package') {
            steps {
                echo 'Packaging.'
            }
        }//Package
    }//stages
}//pipeline
